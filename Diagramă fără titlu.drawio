<!-- Copiază acest cod în diagrams.net (File → Open Library → Advanced → Paste) -->
<mxlibrary>
  <mxGraphModel>
    <root>
      <!-- 1. Adaugă toate tabelele -->
      <mxCell id="Country" value="Country&lt;br&gt;-------------&lt;br&gt;id: INT (PK)&lt;br&gt;name: VARCHAR(100)" style="swimlane;fontSize=12;" vertex="1" parent="1">
        <mxGeometry x="100" y="200" width="120" height="90" as="geometry"/>
      </mxCell>
      
      <mxCell id="Language" value="Language&lt;br&gt;-------------&lt;br&gt;id: INT (PK)&lt;br&gt;name: VARCHAR(100)" style="swimlane;fontSize=12;" vertex="1" parent="1">
        <mxGeometry x="100" y="400" width="120" height="90" as="geometry"/>
      </mxCell>

      <mxCell id="Director" value="Director&lt;br&gt;-------------&lt;br&gt;id: INT (PK)&lt;br&gt;name: VARCHAR(100)" style="swimlane;fontSize=12;" vertex="1" parent="1">
        <mxGeometry x="400" y="100" width="120" height="90" as="geometry"/>
      </mxCell>

      <mxCell id="Genre" value="Genre&lt;br&gt;-------------&lt;br&gt;id: INT (PK)&lt;br&gt;name: VARCHAR(100)" style="swimlane;fontSize=12;" vertex="1" parent="1">
        <mxGeometry x="400" y="500" width="120" height="90" as="geometry"/>
      </mxCell>

      <mxCell id="Movie" value="Movie&lt;br&gt;-------------&lt;br&gt;id: INT (PK)&lt;br&gt;title: VARCHAR(255)&lt;br&gt;release_year: INT&lt;br&gt;duration: INT&lt;br&gt;budget: DECIMAL(15,2)&lt;br&gt;box_office: DECIMAL(15,2)&lt;br&gt;language_id: INT (FK)&lt;br&gt;country_id: INT (FK)&lt;br&gt;director_id: INT (FK)" style="swimlane;fontSize=12;" vertex="1" parent="1">
        <mxGeometry x="250" y="300" width="150" height="180" as="geometry"/>
      </mxCell>

      <mxCell id="MovieGenre" value="MovieGenre&lt;br&gt;-------------&lt;br&gt;movie_id: INT (FK)&lt;br&gt;genre_id: INT (FK)" style="swimlane;fontSize=12;" vertex="1" parent="1">
        <mxGeometry x="400" y="350" width="120" height="90" as="geometry"/>
      </mxCell>

      <!-- 2. Adaugă relațiile -->
      <!-- Director → Movie (1:N) -->
      <mxCell id="rel1" style="edgeStyle=none;endArrow=open;endSize=12;html=1;exitX=0.5;exitY=1;entryX=0.25;entryY=0;" edge="1" parent="1" source="Director" target="Movie">
        <mxGeometry relative="1" as="geometry"/>
      </mxCell>
      <mxCell id="crow1" value="1" style="text;html=1;strokeColor=none;fillColor=none;align=center;verticalAlign=middle;whiteSpace=wrap;" vertex="1" parent="rel1">
        <mxGeometry x="-0.2" y="-20" width="40" height="20" as="geometry"/>
      </mxCell>
      <mxCell id="crow2" value="N" style="text;html=1;strokeColor=none;fillColor=none;align=center;verticalAlign=middle;whiteSpace=wrap;" vertex="1" parent="rel1">
        <mxGeometry x="0.8" y="-20" width="40" height="20" as="geometry"/>
      </mxCell>

      <!-- Country → Movie (1:N) -->
      <mxCell id="rel2" style="edgeStyle=none;endArrow=open;endSize=12;html=1;exitX=1;exitY=0.5;entryX=0;entryY=0.2;" edge="1" parent="1" source="Country" target="Movie">
        <mxGeometry relative="1" as="geometry"/>
      </mxCell>
      <mxCell id="crow3" value="1" style="text;html=1;strokeColor=none;fillColor=none;align=center;verticalAlign=middle;whiteSpace=wrap;" vertex="1" parent="rel2">
        <mxGeometry x="-0.2" relative="1" y="-20" width="40" height="20" as="geometry"/>
      </mxCell>
      <mxCell id="crow4" value="N" style="text;html=1;strokeColor=none;fillColor=none;align=center;verticalAlign=middle;whiteSpace=wrap;" vertex="1" parent="rel2">
        <mxGeometry x="0.8" relative="1" y="-20" width="40" height="20" as="geometry"/>
      </mxCell>

      <!-- Language → Movie (1:N) -->
      <mxCell id="rel3" style="edgeStyle=none;endArrow=open;endSize=12;html=1;exitX=1;exitY=0.5;entryX=0;entryY=0.4;" edge="1" parent="1" source="Language" target="Movie">
        <mxGeometry relative="1" as="geometry"/>
      </mxCell>
      <mxCell id="crow5" value="1" style="text;html=1;strokeColor=none;fillColor=none;align=center;verticalAlign=middle;whiteSpace=wrap;" vertex="1" parent="rel3">
        <mxGeometry x="-0.2" relative="1" y="-20" width="40" height="20" as="geometry"/>
      </mxCell>
      <mxCell id="crow6" value="N" style="text;html=1;strokeColor=none;fillColor=none;align=center;verticalAlign=middle;whiteSpace=wrap;" vertex="1" parent="rel3">
        <mxGeometry x="0.8" relative="1" y="-20" width="40" height="20" as="geometry"/>
      </mxCell>

      <!-- Movie → MovieGenre (1:N) -->
      <mxCell id="rel4" style="edgeStyle=none;endArrow=open;endSize=12;html=1;exitX=1;exitY=0.7;entryX=0;entryY=0.5;" edge="1" parent="1" source="Movie" target="MovieGenre">
        <mxGeometry relative="1" as="geometry"/>
      </mxCell>
      <mxCell id="crow7" value="1" style="text;html=1;strokeColor=none;fillColor=none;align=center;verticalAlign=middle;whiteSpace=wrap;" vertex="1" parent="rel4">
        <mxGeometry x="-0.2" relative="1" y="-20" width="40" height="20" as="geometry"/>
      </mxCell>
      <mxCell id="crow8" value="N" style="text;html=1;strokeColor=none;fillColor=none;align=center;verticalAlign=middle;whiteSpace=wrap;" vertex="1" parent="rel4">
        <mxGeometry x="0.8" relative="1" y="-20" width="40" height="20" as="geometry"/>
      </mxCell>

      <!-- Genre → MovieGenre (1:N) -->
      <mxCell id="rel5" style="edgeStyle=none;endArrow=open;endSize=12;html=1;exitX=0.5;exitY=0;entryX=0.5;entryY=1;" edge="1" parent="1" source="Genre" target="MovieGenre">
        <mxGeometry relative="1" as="geometry"/>
      </mxCell>
      <mxCell id="crow9" value="1" style="text;html=1;strokeColor=none;fillColor=none;align=center;verticalAlign=middle;whiteSpace=wrap;" vertex="1" parent="rel5">
        <mxGeometry x="-0.2" relative="1" y="-20" width="40" height="20" as="geometry"/>
      </mxCell>
      <mxCell id="crow10" value="N" style="text;html=1;strokeColor=none;fillColor=none;align=center;verticalAlign=middle;whiteSpace=wrap;" vertex="1" parent="rel5">
        <mxGeometry x="0.8" relative="1" y="-20" width="40" height="20" as="geometry"/>
      </mxCell>
    </root>
  </mxGraphModel>
</mxlibrary>
